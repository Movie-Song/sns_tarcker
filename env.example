# Notion API 키 (https://www.notion.so/my-integrations에서 생성)
NOTION_API_KEY=your_notion_api_key_here

# Notion 데이터베이스 ID (데이터베이스 URL에서 찾을 수 있음)
NOTION_DATABASE_ID=your_notion_database_id_here

# 티스토리 블로그 RSS 피드 URL
RSS_FEED_URL=https://yourblog.tistory.com/rss

# 선택적: 로깅 레벨 설정 (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO
